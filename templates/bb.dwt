<!doctype html>
<html lang="{{lang}}" class=" no-js">
<head>
    <meta charset="utf-8">
    
	<!-- TemplateBeginEditable name="doctitle" -->
    <title>{{ title }}</title>
    <!-- TemplateEndEditable -->
    
	<script>
		document.documentElement.className = 'js';
			
		document.createElement('aside');
		document.createElement('article');
		document.createElement('footer');
		document.createElement('header');
		document.createElement('hgroup');
		document.createElement('menu');
		document.createElement('nav');
		document.createElement('section');
	</script>
	<style>
		#wrapper {
			margin-top: 0px;
			margin-right: auto;
			margin-bottom: 0px;
			margin-left: auto;
			padding: 0px;
			width: 900px;
		}
		h2 {
			font-family: Georgia, "Times New Roman", Times, serif;
			font-size: 90%;
			color: #979392;
			letter-spacing: 1px;
		}
		h1 {
			font-family: Georgia, "Times New Roman", Times, serif;
			font-size: 100%;
			font-weight: bold;
			text-transform: uppercase;
			color: #020202;
		}
		.inline {
			display: inline;
		}
		body,td,th {
			font-family: Georgia, "Times New Roman", Times, serif;
			color: #4b4b4b;
			display: inline;
			list-style-position: outside;
			list-style-type: none;
		}
		body {
			background-color: #fff;
			margin-left: 0px;
			margin-right: 0px;
		}
		a:link {
			color: #999;
			text-decoration: none;
		}
		a:visited {
			color: #abc;
			text-decoration: none;
		}
		a:hover {
			text-decoration: underline;
		}
		a:active {
			text-decoration: none;
		}
    </style>
	<!-- TemplateBeginEditable name="head" -->
    <!-- TemplateEndEditable -->
</head>
<body>
    <div id="wrapper" class="[{{class1 class2 class3}}]" >
        <header>
		<!-- TemplateBeginEditable name="header" -->
            <h1> {{title.h1}} </h1>
            <img src="../images/400x100-logo.png" alt ="{{ alternative-text }}" >
            <hgroup>
                <h1> {{title.h1}} </h1>
                <h2> {{title.h2}} </h2>
                <h3> {{title.h3}} </h3>
            </hgroup>
            <img src="../images/400x100-logo.png" alt ="{{ alternative-text-2 }}" > <!-- TemplateEndEditable -->
        </header>
        <!-- TemplateEndEditable -->
        
        <!-- TemplateBeginEditable name="nav" -->
        <hr>
        <nav>
            <ul>
                <li> <a href="#" class={{"link-one nav.ul.li.a"}} title={{"title.a"}}> {{link.bate}} </a> </li>
                <li> <a href="#" class={{"link-two nav.ul.li.a"}} title={{"title.b"}}> {{link.two}} </a> </li>
                <li> <a href="#" class={{"link-three nav.ul.li.a"}} title={{"title.b"}}> {{link.three}} </a> </li>
            </ul>
        </nav>
        <!-- TemplateEndEditable -->
        <hr>
        
        <!-- [ corporate template ] -->
        
        <div id="{{id}}" class="our-mission [{{class1 class2 class3}}]">
            <section class="corporate-slideshow">
            <!-- TemplateBeginEditable name="section" -->
                <header>
                    <h1> {{section-title.h1}} </h1>
                </header>
                <figure> 
                    <img src="../images/330x440-ad.png" alt ="{{ slideshow }}" > <img src="{{ /path-to/.img}}" alt ="{{ slideshow }}" > <img src="{{ /path-to/.img}}" alt ="{{ slideshow }}" >
                    <figcaption>
                        <ol>
                            <li>{{slideshow-caption-text-1}}</li>
                            <li>{{slideshow-caption-text-2}}</li>
                            <li>{{slideshow-caption-text-3}}</li>
                        </ol>
                    </figcaption>
                </figure>
                <nav>
                    <ul id="{{id}}" class="{{mission vision values}}">
                        <li> <a href="#" class={{"a.nav.ul.li.slideshow"}} title={{"title.a.slideshow1"}}> {{link.image.one}} </a></li>
                        <li> <a href="#" class={{"a.nav.ul.li.slideshow"}} title={{"title.a.slideshow2"}}> {{link.image.two}} </a></li>
                        <li> <a href="#" class={{"a.nav.ul.li.slideshow"}} title={{"title.a.slideshow3"}}> {{link.image.two}} </a></li>
                    </ul>
                </nav>
             </section>
            <!-- TemplateEndEditable -->
        </div>
        <!-- [ end corporate template ]  //-->
        <hr>
        <!-- TemplateBeginEditable name="footer" -->
        <footer>
            <nav>
                <ul>
                    <li> {{link.one}}</li>
                    <li> {{link.two}} </li>
                    <li> {{link.three}} </li>
                </ul>
            </nav>
            <address>
            	c/ Tuset 5
            </address>
        </footer>
        <!-- TemplateEndEditable -->
  </div>
</body>
</html>